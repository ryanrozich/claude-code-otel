      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#F2495C", "value": null},
              {"color": "#FADE2A", "value": 50},
              {"color": "#73BF69", "value": 80}
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "reduceOptions": {"calcs": ["lastNotNull"], "values": false, "fields": ""},
