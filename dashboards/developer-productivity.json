      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [{
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "expr": "100 * sum(increase(claude_code_token_usage_tokens_total{job=\"otel-collector\",type=\"cacheRead\"}[$__range])) / (sum(increase(claude_code_token_usage_tokens_total{job=\"otel-collector\",type=\"cacheRead\"}[$__range])) + sum(increase(claude_code_token_usage_tokens_total{job=\"otel-collector\",type=\"input\"}[$__range])))",
        "legendFormat": "Cache %",
        "refId": "A"
